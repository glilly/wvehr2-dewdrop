^XWB(8994,2179,2,3,1,9,0)="      RemoteProcedure:= 'RORHEPC PATIENT LOAD';"
^XWB(8994,2179,2,3,1,10,0)="      Param[0].Value := RegistryIEN;"
^XWB(8994,2179,2,3,1,11,0)="      Param[0].PType := literal;"
^XWB(8994,2179,2,3,1,12,0)="      Param[1].Value := DFN;"
^XWB(8994,2179,2,3,1,13,0)="      Param[1].PType := literal;"
^XWB(8994,2179,2,3,1,14,0)="      lstCall(PatientData);     // Load the data"
^XWB(8994,2179,2,3,1,15,0)=" "
^XWB(8994,2179,2,3,1,16,0)="      ...                       // Edit the data"
^XWB(8994,2179,2,3,1,17,0)=" "
