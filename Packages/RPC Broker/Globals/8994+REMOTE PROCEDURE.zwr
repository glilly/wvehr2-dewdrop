^XWB(8994,1772,2,3,1,13,0)="  RPCBroker.Param[1].PType := literal;"
^XWB(8994,1772,2,3,1,14,0)="  RPCBroker.Param[2].Value := '5,5,400,250';"
^XWB(8994,1772,2,3,1,15,0)="  RPCBroker.Param[2].PType := literal;"
^XWB(8994,1772,2,3,1,16,0)=" "
