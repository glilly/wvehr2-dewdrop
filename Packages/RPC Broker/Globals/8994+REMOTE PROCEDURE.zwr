^XWB(8994,2431,0)="VBECS DSS EXTRACT^EN^VBECDSS^4^R"
^XWB(8994,2431,1,0)="^8994.01^4^4^3071029^^^^"
^XWB(8994,2431,1,1,0)="This RPC inserts or updates post transfusion related data in the VBECS DSS"
^XWB(8994,2431,1,2,0)="EXTRACT file (#6002.03). The data is passed into the VBECDSS routine "
^XWB(8994,2431,1,3,0)="through the input parameters and a success indicator is returned to the "
^XWB(8994,2431,1,4,0)="Blood Bank medical device."
^XWB(8994,2431,2,0)="^8994.02A^2^1"
^XWB(8994,2431,2,2,0)="PARAMS^1^999^1^1"
^XWB(8994,2431,2,2,1,0)="^^26^26^3071029^"
^XWB(8994,2431,2,2,1,1,0)="PARAMS(""TRANSACTION ID"") = Unique record identifier"
^XWB(8994,2431,2,2,1,2,0)="PARAMS(""DFN"") = Patient identifier"
^XWB(8994,2431,2,2,1,3,0)="PARAMS(""ORDERING LOCATION"") = Ordering Institution Identifier"
^XWB(8994,2431,2,2,1,4,0)="PARAMS(""TRANSFUSION LOCATION"") = Tranfusing Institution identifier"
^XWB(8994,2431,2,2,1,5,0)="PARAMS(""PHYSICIAN"") = Provider requesting blood product for transfusion"
^XWB(8994,2431,2,2,1,6,0)="PARAMS(""ORDERING PROVIDER"") = Provider who ordered Type and Crossmatch"
^XWB(8994,2431,2,2,1,7,0)="PARAMS(""PRODUCT NAME"") = Short blood product name"
^XWB(8994,2431,2,2,1,8,0)="PARAMS(""COMPONENT ABBREVIATION"") = Abbreviation of blood component"
^XWB(8994,2431,2,2,1,9,0)="PARAMS(""NUMBER OF UNITS"") = Number of pooled units transfused"
^XWB(8994,2431,2,2,1,10,0)="PARAMS(""TRANSFUSION DATE"") = Date/time of transfusion"
^XWB(8994,2431,2,2,1,11,0)="PARAMS(""VOLUME"") = Total volume of units transfused"
^XWB(8994,2431,2,2,1,12,0)="PARAMS(""REACTION TYPE"") = Type of reaction indicated"
^XWB(8994,2431,2,2,1,13,0)="PARAMS(""UNIT MODIFICATION"") = String of codes representing modifications "
^XWB(8994,2431,2,2,1,14,0)="   done on units transfused. String cannot exceed 6 character. "
^XWB(8994,2431,2,2,1,15,0)="   D = Deglycerolize"
^XWB(8994,2431,2,2,1,16,0)="   F = Freeze"
^XWB(8994,2431,2,2,1,17,0)="   I = Irradiate"
^XWB(8994,2431,2,2,1,18,0)="   L = Leukoreduce"
^XWB(8994,2431,2,2,1,19,0)="   P = Pool"
^XWB(8994,2431,2,2,1,20,0)="   R = Rejuvenate"
^XWB(8994,2431,2,2,1,21,0)="   S = Split/Divide"
^XWB(8994,2431,2,2,1,22,0)="   T = Thaw"
^XWB(8994,2431,2,2,1,23,0)="   U = Thaw/Pool Cryo"
^XWB(8994,2431,2,2,1,24,0)="   V = Volume Reduce"
^XWB(8994,2431,2,2,1,25,0)="   W = Wash"
^XWB(8994,2431,2,2,1,26,0)="PARAMS(""REACTION"") = Yes or No value if a reaction was indicated."
^XWB(8994,2431,2,"B","PARAMS",2)=""
^XWB(8994,2431,2,"PARAMSEQ",1,2)=""
^XWB(8994,2431,3,0)="^8994.03^9^9^3071029^^^^"
^XWB(8994,2431,3,1,0)="This RPC returns and XML document containing a SuccessIndicator element "
^XWB(8994,2431,3,2,0)="represented by either a 1 for a successful insert or update or a 0 for an "
^XWB(8994,2431,3,3,0)="unsuccessful insert or update in the VBECS DSS EXTRACT file (#6002.03)."
^XWB(8994,2431,3,4,0)=" "
^XWB(8994,2431,3,5,0)="Example of successful transaction:"
^XWB(8994,2431,3,6,0)="<ReturnValue><SuccessIndicator>1</SuccessIndicator></ReturnValue>"
^XWB(8994,2431,3,7,0)=" "
^XWB(8994,2431,3,8,0)="Example of unsuccessful transaction:"
^XWB(8994,2431,3,9,0)="<ReturnValue><SuccessIndicator>0</SuccessIndicator></ReturnValue>"
