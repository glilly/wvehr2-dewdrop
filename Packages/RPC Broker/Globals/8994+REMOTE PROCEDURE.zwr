^XWB(8994,1277,3,12,0)="        piece3 = vital type name (FILE 120.51, Field .01)"
^XWB(8994,1277,3,13,0)="        piece4 = Abbreviation (FILE 120.51, Field 1)"
^XWB(8994,1277,3,14,0)="        piece5 = PCE Abbreviation (FILE 120.51, Field 7)"
