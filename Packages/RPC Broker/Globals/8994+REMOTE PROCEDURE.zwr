^XWB(8994,2426,1,3,0)=" "
^XWB(8994,2426,1,4,0)="The patient demographic data is provided by the VistA API DEM^VADPT in "
^XWB(8994,2426,1,5,0)="the VADM array."
^XWB(8994,2426,1,6,0)=" "
^XWB(8994,2426,1,7,0)="The medication related data is provided by the Pharmacy API OCL^PSOORRL in"
^XWB(8994,2426,1,8,0)="the ^TMP(""PS"",$J global array in the following format."
^XWB(8994,2426,1,9,0)=" "
^XWB(8994,2426,1,10,0)="^TMP(""PS"",$J,D0,0)=ORDER #_File;PKG^DRUG^INFUSION RATE^STOP DATE^REFILLS"
^XWB(8994,2426,1,11,0)="REMAINING^TOTAL DOSE^UNITS PER DOSE^PLACER #^STATUS^LAST FILLDATE^DAYS"
^XWB(8994,2426,1,12,0)="SUPPLY^QUANTITY^NOT TO BE GIVEN^PENDING RENEWAL (1 or 0)^ISSUE"
^XWB(8994,2426,1,13,0)="DATE(Outpatient) or START DATE(Inpatient)^LAST RELEASE DATE/TIME^LAST FILL"
^XWB(8994,2426,1,14,0)="DAY SUPPLY"
^XWB(8994,2426,1,15,0)=" "
^XWB(8994,2426,1,16,0)="XML Mapping:"
