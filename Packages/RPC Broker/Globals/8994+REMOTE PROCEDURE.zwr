^XWB(8994,2426,1,18,0)="PatientSsn element =           VADM(2)"
^XWB(8994,2426,1,19,0)="PatientDob element =           VADM(3) in HL7 format"
^XWB(8994,2426,1,20,0)="BeginningSearchDate element =  SDATE input parameter in HL7 format"
^XWB(8994,2426,1,21,0)="EndingSearchDate element =     EDATE input parameter in HL7 format"
^XWB(8994,2426,1,22,0)="OrderNumber element =          ORDER #"
^XWB(8994,2426,1,23,0)="DrugName element =             DRUG"
^XWB(8994,2426,1,24,0)="IssueStartDate element =       ISSUE DATE(Outpatient) or START "
^XWB(8994,2426,1,25,0)="                               DATE(Inpatient)"
^XWB(8994,2426,1,26,0)="Status element =               STATUS"
^XWB(8994,2426,1,27,0)="InpatientOutpatientIndicator = PKG"
^XWB(8994,2426,1,28,0)=" "
