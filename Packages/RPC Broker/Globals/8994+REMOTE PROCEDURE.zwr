^XWB(8994,2179,2,3,1,23,0)="      Param[2].PType := list;"
^XWB(8994,2179,2,3,1,24,0)="      Param[2].Mult.Assign(PatientData);"
^XWB(8994,2179,2,3,1,25,0)="      Call;                     // Save the data"
^XWB(8994,2179,2,3,1,26,0)="   end;"
^XWB(8994,2179,2,3,1,27,0)=" "
