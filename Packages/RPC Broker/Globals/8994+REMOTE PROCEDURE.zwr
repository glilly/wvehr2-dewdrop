^XWB(8994,1266,2,0)="^8994.02A^1^1"
^XWB(8994,1266,2,1,0)="GMRDATE^1^30^1^1"
^XWB(8994,1266,2,1,1,0)="^8994.021^1^1^3051011^^^^"
